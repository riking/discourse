"use strict";
self.Discourse = {SiteSettings:{}};
self.BaseUri = '<%= Discourse.base_uri %>';
self.CDN = '<%= Rails.configuration.action_controller.asset_host %>';
self.Discourse.Environment = '<%= Rails.env %>';
self.Discourse.SiteSettings.long_polling_base_url = '<%= SiteSetting.long_polling_base_url %>';

<% if Rails.env.development?
  message_bus_root = File.join(Rails.root, '../message_bus')
  if Dir.exists? message_bus_root
    mbus_worker = File.join(message_bus_root, 'assets/message-bus/worker.js')
    depend_on mbus_worker
    %>
    let VersionNumber = '<%= File.mtime(mbus_worker) %>';
  <% end %>
<% end %>
